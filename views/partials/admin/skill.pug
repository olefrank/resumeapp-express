.well
    .row
        .col-xs-6
            .form-group
                label Titel
                input.admin-skill-title.form-control(data-prop="title.da", data-type="resume-form-field", data-section="skill", data-id=item._id, value=item.title.da)
        .col-xs-6
            .form-group
                label Title
                input.admin-skill-title.form-control(data-prop="title.en", data-type="resume-form-field", data-section="skill", data-id=item._id, value=item.title.en)
    .row
        .col-xs-6
            .form-group
                label Rating
                input.admin-skill-company.form-control(type="number", min=1, max=5, data-prop="rating", data-type="resume-form-field", data-section="skill", data-id=item._id, value=item.rating)

        .col-xs-6
            button.btn.btn-success.btn-admin-save(type='button', data-section='skill', data-id=item._id)= strings.buttons.save.da